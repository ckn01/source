{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "name": "Next.js: dev (node-terminal)",
      "request": "launch",
      "command": "npm run dev",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Next.js",
      "port": 9229,
      "restart": true,
      "protocol": "inspector",
      "timeout": 10000,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}